FUNCTION insertionsort(array_of INTEGER [n])
VAR
    i,j: INTEGER
BEGIN
    int n=arr.length;
    FOR (i=1; i<n);
    int key=arr[i];
    int j=i-1;
    END FOR

    while (j>=0 and arr[j]>i);
    arr[j+1]= arr[j];

    (arr[j], arr[j-1])= (arr[j-1], arr[j]);
    THEN 
    return arr;

END